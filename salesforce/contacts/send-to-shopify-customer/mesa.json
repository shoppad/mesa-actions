{
  "name": "Send Salesforce Contact to Shopify Customer",
  "version": "1.0.0",
  "description": "Sends a Salesforce contact to a Shopify customer record.",
  "url": "shoppad/mesa-templates/salesforce/contacts/send-to-shopify-customer",
  "documentation": "https://github.com/shoppad/mesa-templates/blob/master/salesforce/contacts/send-to-shopify-customer/README.md",
  "tags": ["customers", "salesforce"],
  "source": "salesforce",
  "destination": "shopify",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "json_webhook",
        "name": "Send Salesforce Contact to Shopify Customer",
        "key": "in-send-salesforce-contact-to-shopify",
        "trigger_name": "In: Send Salesforce Contact To Shopify",
        "script": "salesforce/contacts/send-to-shopify-customer/input.js"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Send Salesforce Contact to Shopify Customer",
        "key": "out-send-salesforce-contact-to-shopify",
        "trigger_name": "Out: Send Salesforce Contact To Shopify",
        "script": "salesforce/contacts/send-to-shopify-customer/output.js"
      }
    ],
    "secrets": [
      {
        "name": "salesforce_client_id"
      },
      {
        "name": "salesforce_client_secret"
      },
      {
        "name": "salesforce_username"
      },
      {
        "name": "salesforce_password"
      }
    ]
  },
  "files": [
    "salesforce/contacts/send-to-shopify-customer/input.js",
    "salesforce/contacts/send-to-shopify-customer/output.js",
    "salesforce/contacts/send-to-shopify-customer/shopify-salesforce-customer-map.js"
  ]
}