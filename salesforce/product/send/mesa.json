{
  "key": "shoppad/mesa-templates/salesforce/product/send",
  "name": "Send Product To Shopify Product",
  "version": "1.0.0",
  "description": "Send product from Salesforce to Shopify Product when product is created.",
  "tags": [
    "CRM",
    "Product"
  ],
  "source": "salesforce",
  "destination": "shopify",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "json_webhook",
        "name": "Salesforce Product Create",
        "key": "in-salesforce-product-create",
        "script": "in-salesforce-product-create.js",
        "topic": "orders/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "shopify_api",
        "name": "Create Shopify Product",
        "key": "out-create-shopify-product",
        "script": "out-create-shopify-product.js",
        "shopify_api": "POST admin/products.json"
      }
    ],
    "secrets": [
      {
        "key": "salesforce.oauth"
      }
    ],
    "storage": [
      {
        "key": "salesforce-instance",
        "value": "https://na19.salesforce.com"
      }
    ]
  }
}