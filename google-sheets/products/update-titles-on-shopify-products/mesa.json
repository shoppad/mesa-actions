{
  "name": "Sync product title updates Google Sheets to Shopify",
  "version": "1.0.0",
  "description": "",
  "url": "shoppad/google-sheets/products/update-titles-on-shopify-products",
  "documentation":
    "https://github.com/shoppad/mesa-templates/blob/master/google-sheets/products/update-titles-on-shopify-products/README.md",
  "tags": ["products", "google-sheets"],
  "source": "google-sheets",
  "destination": "shopify",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "custom",
        "name": "products google",
        "key": "in-products-google",
        "trigger_name": "In: products google",
        "schedule": "0 * * * *",
        "script":
          "google-sheets/products/update-titles-on-shopify-products/in/in-products-google.js"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "products google",
        "key": "out-products-google",
        "trigger_name": "Out: products google",
        "script":
          "google-sheets/products/update-titles-on-shopify-products/out-products-google.js"
      }
    ],
    "secrets": [
      {
        "name": "google_refresh_token"
      },
      {
        "name": "google_access_token"
      },
      {
        "name": "google_client_id"
      },
      {
        "name": "google_client_secret"
      },
      {
        "name": "google_expires_at"
      }
    ],
    "storage": [
      {
        "name": "google_sheets_products_id",
        "value": ""
      }
    ]
  },
  "files": [
    "google-sheets/products/update-titles-on-shopify-products/in-products-google.js",
    "google-sheets/products/update-titles-on-shopify-products/out-products-google.js"
  ]
}
