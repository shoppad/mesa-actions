{
  "name": "Sync product title updates Google Sheets to Shopify",
  "version": "1.0.0",
  "description": "",
  "url": "shoppad/google/products/in",
  "documentation":
    "https://github.com/shoppad/mesa-recipes/blob/master/google/products/in/README.md",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "custom",
        "name": "products google",
        "key": "in-products-google",
        "trigger_name": "In: products google",
        "schedule": "0 * * * *",
        "script": "google/products/in/in-products-google.js"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "products google",
        "key": "out-products-google",
        "trigger_name": "Out: products google",
        "script": "google/products/in/out-products-google.js"
      }
    ],
    "secrets": [
      {
        "name": "google_refresh_token"
      },
      {
        "name": "google_access_token"
      },
      {
        "name": "google_client_id"
      },
      {
        "name": "google_client_secret"
      },
      {
        "name": "google_expires_at"
      }
    ],
    "storage": [
      {
        "name": "google_sheets_products_id",
        "value": ""
      }
    ]
  },
  "files": [
    "google/products/in/in-products-google.js",
    "google/products/in/out-products-google.js"
  ]
}
