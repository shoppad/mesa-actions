{
    "key": "send_daily_tiktok_ad_group_report_to_google_sheets",
    "name": "Send daily TikTok Ad Group summary report to Google Sheets",
    "version": "1.0.0",
    "description": "",
    "video": "",
    "tags": [],
    "source": "",
    "destination": "",
    "seconds": 0,
    "enabled": false,
    "logging": false,
    "debug": false,
    "config": {
        "inputs": [
            {
                "schema": 2,
                "trigger_type": "input",
                "type": "custom",
                "name": "Scheduled Trigger",
                "key": "scheduled_trigger",
                "metadata": {
                    "schedule": "@daily:0 8 * * *"
                },
                "local_fields": [],
                "weight": 0
            }
        ],
        "outputs": [
            {
                "schema": 4,
                "trigger_type": "output",
                "type": "tiktok",
                "entity": "reports",
                "action": "ad_group_summary_report",
                "name": "TikTok Reports Ad Group Summary",
                "key": "tiktokmarketing_reports",
                "metadata": {
                    "query": {
                        "advertiser_id": "",
                        "service_type": "AUCTION",
                        "report_type": "BASIC",
                        "data_level": "AUCTION_ADGROUP",
                        "dimensions": "adgroup_id",
                        "metrics": "spend,impressions,reach,clicks,conversion,cpm",
                        "start_date": "{{ \"yesterday\" | date: \"%Y-%m-%d\" }}",
                        "end_date": "{{ \"yesterday\" | date: \"%Y-%m-%d\" }}",
                        "order_type": "ASC",
                        "page": "1",
                        "page_size": "1000"
                    }
                },
                "local_fields": [],
                "weight": 0
            },
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "loop",
                "name": "Loop",
                "key": "loop",
                "metadata": {
                    "key": "{{tiktokmarketing_reports.list[]}}"
                },
                "local_fields": [],
                "weight": 1
            },
            {
                "schema": 4,
                "trigger_type": "output",
                "type": "googlesheets",
                "entity": "row",
                "action": "create",
                "name": "Google Sheets Create Row",
                "key": "googlesheets_row_1",
                "metadata": {
                    "path": {
                        "mode": "create",
                        "spreadsheet_id": "",
                        "sheet_name": "Sheet1",
                        "header_row": "first_row"
                    },
                    "body": {
                        "fields": {
                            "adgroup_id": "{{loop.dimensions.adgroup_id}}",
                            "spend": "{{loop.metrics.spend}}",
                            "impressions": "{{loop.metrics.impressions}}",
                            "reach": "{{loop.metrics.reach}}",
                            "clicks": "{{loop.metrics.clicks}}",
                            "conversion": "{{loop.metrics.conversion}}",
                            "cpm": "{{loop.metrics.cpm}}"
                        }
                    }
                },
                "local_fields": [
                    {
                        "key": "body",
                        "type": "object",
                        "fields": [
                            {
                                "key": "fields",
                                "type": "object",
                                "fields": [
                                    {
                                        "key": "adgroup_id",
                                        "label": "adgroup_id",
                                        "type": "text"
                                    },
                                    {
                                        "key": "spend",
                                        "label": "spend",
                                        "type": "text"
                                    },
                                    {
                                        "key": "impressions",
                                        "label": "impressions",
                                        "type": "text"
                                    },
                                    {
                                        "key": "reach",
                                        "label": "reach",
                                        "type": "text"
                                    },
                                    {
                                        "key": "clicks",
                                        "label": "clicks",
                                        "type": "text"
                                    },
                                    {
                                        "key": "conversion",
                                        "label": "conversion",
                                        "type": "text"
                                    },
                                    {
                                        "key": "cpm",
                                        "label": "cpm",
                                        "type": "text"
                                    }
                                ]
                            }
                        ]
                    }
                ],
                "weight": 2
            }
        ],
        "storage": []
    }
}
