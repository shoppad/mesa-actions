{
  "key": "infiniteoptions_order_add_line_items_to_google_sheet_1",
  "name": "Add Infinite Options Line Items to Google Sheet",
  "version": "1.0.0",
  "enabled": false,
  "config": {
      "inputs": [
          {
              "schema": 2,
              "trigger_type": "input",
              "type": "infiniteoptions",
              "entity": "order",
              "action": "created",
              "name": "Infinite Options: Order Created",
              "key": "infinite_options_order_created",
              "metadata": [],
              "local_fields": [],
              "weight": 0
          }
      ],
      "outputs": [
          {
              "schema": 2,
              "trigger_type": "output",
              "type": "iterator",
              "name": "Iterator",
              "key": "iterator",
              "metadata": {
                  "key": "{{infinite_options_order_created.line_items[]}}"
              },
              "local_fields": [],
              "weight": 0
          },
          {
              "schema": 4,
              "trigger_type": "output",
              "type": "googlesheets",
              "entity": "row",
              "action": "create",
              "name": "Google Sheets Row Create",
              "key": "googlesheets_row_1",
              "metadata": {
                  "header_row": "first_row",
                  "body": {
                      "fields": {
                          "Order URL": "https:\/\/{{context.shop.domain}}\/admin\/orders\/{{infinite_options_order_created.order.id}}",
                          "Order Name": "{{infinite_options_order_created.order.name}}",
                          "Email": "{{infinite_options_order_created.order.email}}",
                          "Shipping Name": "{{infinite_options_order_created.order.shipping_address.first_name}} {{infinite_options_order_created.order.shipping_address.last_name}}",
                          "Address": "{{infinite_options_order_created.order.shipping_address.address1}}",
                          "City": "{{infinite_options_order_created.order.shipping_address.city}}",
                          "Province": "{{infinite_options_order_created.order.shipping_address.province}}",
                          "Zip": "{{infinite_options_order_created.order.shipping_address.zip}}",
                          "Country": "{{infinite_options_order_created.order.shipping_address.country}}",
                          "Product Name": "{{iterator.title}}",
                          "Product SKU": "{{iterator.sku}}",
                          "Product Price": "{{iterator.price}}",
                          "infinite_options_1": "{{iterator.fields.infinite_options_1}}",
                          "infinite_options_2": "{{iterator.fields.infinite_options_2}}"
                      }
                  }
              },
              "local_fields": [
                  {
                      "key": "body",
                      "type": "object",
                      "fields": [
                          {
                              "key": "fields",
                              "type": "object",
                              "fields": [
                                  {
                                      "key": "Order URL",
                                      "label": "Order URL",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Order Name",
                                      "label": "Order Name",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Email",
                                      "label": "Email",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Shipping Name",
                                      "label": "Shipping Name",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Address",
                                      "label": "Address",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "City",
                                      "label": "City",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Province",
                                      "label": "Province",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Zip",
                                      "label": "Zip",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Country",
                                      "label": "Country",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Product Name",
                                      "label": "Product Name",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Product SKU",
                                      "label": "Product SKU",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "Product Price",
                                      "label": "Product Price",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "infinite_options_1",
                                      "label": "infinite_options_1",
                                      "type": "text",
                                      "source": "googlesheets"
                                  },
                                  {
                                      "key": "infinite_options_2",
                                      "label": "infinite_options_2",
                                      "type": "text",
                                      "source": "googlesheets"
                                  }
                              ]
                          }
                      ]
                  }
              ],
              "weight": 1
          }
      ],
      "storage": []
  }
}