{
  "name": "Sync product title updates Google Sheets to Shopify",
  "version": "1.0.0",
  "description": "",
  "url": "shoppad/google/customer/out",
  "documentation":
    "https://github.com/shoppad/mesa-recipes/blob/master/google/customer/out/README.md",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Customer Update",
        "key": "in-customer-update",
        "trigger_name": "In: Customer Update",
        "script": "google/customer/out/in-customer-update.js",
        "topic": "customers/update"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Google Sheets Update",
        "key": "out-google-sheets-update",
        "trigger_name": "Out: Google Sheets Update",
        "script": "google/customer/out/out-google-sheets-update.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Google Sheets Read",
        "key": "out-google-sheets-read",
        "trigger_name": "Out: Google Sheets Read",
        "script": "google/customer/out/out-google-sheets-read.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Customer Update",
        "key": "out-customer-update",
        "trigger_name": "Out: Customer Update",
        "script": "google/customer/out/out-customer-update.js"
      }
    ],
    "secrets": [
      {
        "name": "google_refresh_token"
      },
      {
        "name": "google_access_token"
      },
      {
        "name": "google_client_id"
      },
      {
        "name": "google_client_secret"
      },
      {
        "name": "google_expires_at"
      }
    ]
  },
  "files": [
    "google/customer/out/in-customer-update.js",
    "google/customer/out/out-customer-update.js",
    "google/customer/out/out-google-sheets-read.js",
    "google/customer/out/`out-google-sheets-update.js"
  ]
}
