                "action": "created",
                "name": "Order Created",
                "key": "shopify",
                "operation_id": "orders_create",
                "metadata": [],
                "on_error": "default",
                "weight": 0
            }
        ],
        "outputs": [
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "delay",
