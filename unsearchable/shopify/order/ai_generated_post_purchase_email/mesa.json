            {
                "schema": 4,
                "trigger_type": "output",
                "type": "email",
                "version": "v2",
                "name": "Send Email",
                "key": "email",
                "metadata": {
                    "to": "kalen@getmesa.com",
                    "subject": "{{ai.response}}",
