{
  "name": "Sync customer updates data from Shopify to Google Sheets.",
  "version": "1.0.0",
  "description": "",
  "documentation":
    "https://github.com/shoppad/mesa-recipes/blob/master/shopify/customer/send-to-google-sheets/README.md",
  "tags": ["customers", "google-sheets"],
  "source": "shopify",
  "destination": "google-sheets",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Customer Update",
        "key": "in-customer-update",
        "trigger_name": "In: Customer Update",
        "script":
          "shopify/customer/send-to-google-sheets/in-customer-update.js",
        "topic": "customers/update"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Google Sheets Update",
        "key": "out-google-sheets-update",
        "trigger_name": "Out: Google Sheets Update",
        "script":
          "shopify/customer/send-to-google-sheets/out-google-sheets-update.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Google Sheets Read",
        "key": "out-google-sheets-read",
        "trigger_name": "Out: Google Sheets Read",
        "script":
          "shopify/customer/send-to-google-sheets/out-google-sheets-read.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Customer Update",
        "key": "out-customer-update",
        "trigger_name": "Out: Customer Update",
        "script":
          "shopify/customer/send-to-google-sheets/out-customer-update.js"
      }
    ],
    "secrets": [
      {
        "name": "google_refresh_token"
      },
      {
        "name": "google_access_token"
      },
      {
        "name": "google_client_id"
      },
      {
        "name": "google_client_secret"
      },
      {
        "name": "google_expires_at"
      }
    ],
    "storage": [
      {
        "name": "google_sheets_customers_id",
        "value": ""
      }
    ]
  },
  "files": [
    "shopify/customer/send-to-google-sheets/in-customer-update.js",
    "shopify/customer/send-to-google-sheets/out-customer-update.js",
    "shopify/customer/send-to-google-sheets/out-google-sheets-read.js",
    "shopify/customer/send-to-google-sheets/out-google-sheets-update.js"
  ]
}
