{
  "key": "shoppad/mesa-templates/shopify/customer/send-to-hubspot-deal",
  "name": "Send Customer To Hubspot Deal",
  "version": "1.0.0",
  "description": "Send customer from Shopify to HubSpot Deal when customer is created.",
  "tags": ["Customer"],
  "source": "shopify",
  "destination": "hubspot",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "shopify_webhook",
        "name": "Shopify Customer Create",
        "key": "in-shopify-customer-create",
        "script": "in-shopify-customer-create.js",
        "topic": "customers/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "custom",
        "name": "Create Hubspot Contact",
        "key": "out-create-hubspot-deal",
        "script": "out-create-hubspot-deal.js"
      }
    ],
    "storage": [
      {
        "key": "hubspot_deal_deal_type",
        "value": "newbusiness"
      },
      {
        "key": "hubspot_deal_pipeline",
        "value": "default"
      },
      {
        "key": "hubspot_deal_deal_stage",
        "value": "appointmentscheduled"
      }
    ],
    "secrets": [
      {
        "key": "hubspot.hapi"
      }
    ]
  }
}