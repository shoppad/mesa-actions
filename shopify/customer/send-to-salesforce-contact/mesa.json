{
  "key": "shoppad/mesa-templates/shopify/customer/send-to-salesforce-contact",
  "name": "Send Customer To Salesforce Contact",
  "version": "1.0.0",
  "description": "Send Customer from Shopify to Salesforce contact when customer is created.",
  "tags": ["customer", "salesforce"],
  "source": "shopify",
  "destination": "salesforce",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "shopify_webhook",
        "name": "Shopify Customer Create",
        "key": "in-shopify-customer-create",
        "script": "in-shopify-customer-create.js",
        "topic": "customers/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "custom",
        "name": "Create Salesforce Contact",
        "key": "out-create-salesforce-contact",
        "script": "out-create-salesforce-contact.js"
      }
    ],
    "secrets": [
      {
        "key": "salesforce.oauth",
        "type": "oauth",
        "service": "salesforce"
      }
    ]
  }
}