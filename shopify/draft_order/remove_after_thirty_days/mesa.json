{
    "key": "shopify/draft_order/remove_after_thirty_days",
    "name": "Remove draft orders after thirty days",
    "version": "1.0.0",
    "description": "Keep the draft orders on your online store tidy and current with the help of MESA. This template removes draft orders that have been sitting on your Shopify store for more than 30 days. Now you can keep draft order clutter at bay automatically!",
    "video": "",
    "readme": "",
    "tags": [],
    "source": "shopify_webhook",
    "destination": "shopify_api",
    "seconds": 60,
    "enabled": true,
    "logging": true,
    "debug": false,
    "config": {
        "inputs": [
            {
                "schema": 3,
                "trigger_type": "input",
                "type": "shopify_webhook",
                "entity": "draft_order",
                "action": "created",
                "name": "Shopify Draft Order Created",
                "key": "shopify_draft_order",
                "metadata": [],
                "weight": 0
            }
        ],
        "outputs": [
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "delay",
                "name": "Delay for 30 days",
                "key": "delay",
                "metadata": {
                    "amount": "30",
                    "unit": "days",
                    "test_bypass": false
                },
                "local_fields": [],
                "weight": 0
            },
            {
                "schema": 3,
                "trigger_type": "output",
                "type": "shopify_api",
                "entity": "draft_order",
                "action": "delete",
                "name": "Shopify Delete Draft Order",
                "key": "shopify_draft_order_1",
                "metadata": {
                    "draft_order_id": "{{delay.id}}"
                },
                "local_fields": [],
                "weight": 1
            }
        ],
        "storage": []
    }
}