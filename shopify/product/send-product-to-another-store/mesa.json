{
  "name": "Send Product To Another Store",
  "version": "1.0.0",
  "description": "Send product from Shopify to another store when product is created, updated or deleted.",
  "tags": ["products"],
  "source": "shopify",
  "destination": "shopify",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Send Product To Another Store: Create",
        "key": "in-send-created-products-to-another-store",
        "trigger_name": "In: Send Product To Another Store",
        "script": "in-output-send.js",
        "topic": "products/create"
      },
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Send Product To Another Store: Update",
        "key": "in-send-updated-products-to-another-store",
        "trigger_name": "In: Send Product To Another Store",
        "script": "in-output-send.js",
        "topic": "products/update"
      },
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Send Product To Another Store: Remove",
        "key": "in-send-deleted-products-to-another-store",
        "trigger_name": "In: Send Product To Another Store",
        "script": "in-output-send.js",
        "topic": "products/delete"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Send Product To Another Store: Create",
        "key": "out-send-created-products-to-another-store",
        "trigger_name": "Out: Send Product To Another Store",
        "script": "shopify/product/send-product-to-another-store/output-create.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Send Product To Another Store: Update",
        "key": "out-send-updated-products-to-another-store",
        "trigger_name": "Out: Send Product To Another Store",
        "script": "shopify/product/send-product-to-another-store/output-update.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Send Product To Another Store: Remove",
        "key": "out-send-deleted-products-to-another-store",
        "trigger_name": "Out: Send Product To Another Store",
        "script": "shopify/product/send-product-to-another-store/output-delete.js"
      }
    ]
  },
  "secrets": [
    {
      "name": "shopify/product/send-product-to-another-store/password"
    }
  ],
  "storage": [
    {
      "name": "shopify/product/send-product-to-another-store/host"
    },
    {
      "name": "shopify/product/send-product-to-another-store/key"
    },
    {
      "name": "shopify/product/send-product-to-another-store/product-map.json",
      "value": "{}"
    }
  ],
  "files": [
    "shopify/product/send-product-to-another-store/output-create.js",
    "shopify/product/send-product-to-another-store/output-update.js",
    "shopify/product/send-product-to-another-store/output-delete.js"
  ]

}