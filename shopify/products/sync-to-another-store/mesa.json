{
  "name": "Sync Products to Another Shopify Store",
  "version": "1.0.0",
  "description": "",
  "tags": ["products"],
  "source": "shopify",
  "destination": "shopify",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Sync created products to another store",
        "key": "in-sync-created-products-to-another-store",
        "trigger_name": "In: Sync created products to another store",
        "script": "in-output-send.js",
        "topic": "products/create"
      },
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Sync updated products to another store",
        "key": "in-sync-updated-products-to-another-store",
        "trigger_name": "In: Sync updated products to another store",
        "script": "in-output-send.js",
        "topic": "products/update"
      },
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Sync deleted products to another store",
        "key": "in-sync-deleted-products-to-another-store",
        "trigger_name": "In: Sync deleted products to another store",
        "script": "in-output-send.js",
        "topic": "products/delete"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Sync created products to another store",
        "key": "out-sync-created-products-to-another-store",
        "trigger_name": "Out: Sync created products to another store",
        "script": "shopify/products/sync-to-another-store/output-create.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Sync updated products to another store",
        "key": "out-sync-updated-products-to-another-store",
        "trigger_name": "Out: Sync updated products to another store",
        "script": "shopify/products/sync-to-another-store/output-update.js"
      },
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Sync deleted products to another store",
        "key": "out-sync-deleted-products-to-another-store",
        "trigger_name": "Out: Sync deleted products to another store",
        "script": "shopify/products/sync-to-another-store/output-delete.js"
      }
    ]
  },
  "secrets": [
    {
      "name": "shopify/products/sync-with-another-store/password"
    }
  ],
  "storage": [
    {
      "name": "shopify/products/sync-with-another-store/host"
    },
    {
      "name": "shopify/products/sync-with-another-store/key"
    }
  ],
  "files": [
    "shopify/products/sync-to-another-store/output-create.js",
    "shopify/products/sync-to-another-store/output-update.js",
    "shopify/products/sync-to-another-store/output-delete.js"
  ]

}