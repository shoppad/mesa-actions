{
  "key": "shoppad/mesa-templates/shopify/order/send-uploadery-file-to-dropbox",
  "name": "Send Uploadery File To Dropbox",
  "version": "1.0.0",
  "description": "Send Uploadery file from Shopify order to Dropbox when order is created.",
  "tags": ["order", "file", "dropbox"],
  "source": "uploadery",
  "destination": "dropbox",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "shopify_webhook",
        "name": "Shopify Order Create",
        "key": "in-shopify-order-create",
        "script": "in-shopify-order-create.js",
        "topic": "orders/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "custom",
        "name": "Send to Dropbox",
        "key": "out-send-to-dropbox",
        "script": "out-send-to-dropbox.js"
      }
    ],
    "secrets": [
      {
        "key": "dropbox-client-id"
      },
      {
        "key": "dropbox-client-secret"
      },
      {
        "key": "dropbox-authorize-url"
      },
      {
        "key": "dropbox-token-url"
      },
      {
        "key": "dropbox-access-token"
      }
    ],
    "storage": [
      {
        "key": "field-key",
        "value": "uploadery_1"
      }
    ]
  }
}