{
  "key": "shoppad/mesa-templates/shopify/order/send-uploadery-file-to-cloudinary-transform",
  "name": "Send Uploadery File To Cloudinary Transform",
  "version": "1.0.0",
  "description": "Send Uploadery file from Shopify order to Cloudinary Transform when order is created.",
  "tags": ["Order"],
  "source": "uploadery",
  "destination": "cloudinary",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "shopify_webhook",
        "name": "Shopify Order Create",
        "key": "in-shopify-order-create",
        "script": "in-shopify-order-create.js",
        "topic": "orders/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "custom",
        "name": "Send to Cloudinary",
        "key": "out-send-to-cloudinary",
        "script": "out-send-to-cloudinary.js"
      },
      {
        "trigger_type": "output",
        "type": "shopify_api",
        "name": "Update Shopify Order Notes",
        "key": "out-update-shopify-order-notes",
        "script": "out-update-shopify-order-notes.js",
        "shopify_api": "PUT admin/orders/{{order_id}}.json"
      }
    ],
    "secrets": [
      {
        "key": "cloudinary-api-key"
      },
      {
        "key": "cloudinary-api-secret"
      }
    ],
    "storage": [
      {
        "key": "cloudinary-cloudname",
        "value": ""
      },
      {
        "key": "lineitem-property-key",
        "value": "uploadery_1"
      },
      {
        "key": "notes-attribute-name",
        "value": "cloudinary_image"
      },
      {
        "key": "cloudinary-transformation",
        "value": ""
      }
    ]
  }
}