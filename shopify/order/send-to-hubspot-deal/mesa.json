{
  "key": "shoppad/mesa-templates/shopify/order/send-to-hubspot-deal",
  "name": "Send Order To HubSpot Deal",
  "version": "1.0.0",
  "description": "Send order from Shopify to HubSpot Deal when order is created.",
  "tags": ["Order"],
  "source": "shopify",
  "destination": "hubspot",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "shopify_webhook",
        "name": "Shopify Order Create",
        "key": "in-shopify-order-create",
        "script": "in-shopify-order-create.js",
        "topic": "orders/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "custom",
        "name": "Create Hubspot Deal",
        "key": "out-create-hubspot-deal",
        "script": "out-create-hubspot-deal.js"
      }
    ],
    "storage": [
      {
        "key": "hubspot_deal_deal_type",
        "value": "newbusiness"
      },
      {
        "key": "hubspot_deal_pipeline",
        "value": "default"
      },
      {
        "key": "hubspot_deal_deal_stage",
        "value": "closedwon"
      },
      {
        "key": "hubspot_create_deal_without_contact",
        "value": "true"
      }
    ],
    "secrets": [
      {
        "key": "hubspot.hapi"
      }
    ]
  }
}