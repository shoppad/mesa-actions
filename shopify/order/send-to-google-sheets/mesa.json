{
  "name": "Send orders creation data from Shopify to Google Sheets",
  "version": "1.0.0",
  "description": "Send",
  "url": "shoppad/mesa-templates/shopify/order/send-to-google-sheets",
  "documentation":
    "https://github.com/shoppad/mesa-templates/blob/master/shopify/order/send-to-google-sheets/README.md",
  "tags": ["order", "google-sheets"],
  "source": "shopify",
  "destination": "google-sheets",
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "enabled": true,
        "type": "shopify_webhook",
        "name": "Orders Google Sheets",
        "key": "in-orders-google-sheets",
        "trigger_name": "In: Orders Google Sheets",
        "script":
          "shopify/order/send-to-google-sheets/in-orders-google-sheets.js",
        "topic": "orders/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "enabled": true,
        "type": "custom",
        "name": "Orders Google Sheets",
        "key": "out-orders-google-sheets",
        "trigger_name": "Out: Orders Google Sheets",
        "script":
          "shopify/order/send-to-google-sheets/out-orders-google-sheets.js"
      }
    ],
    "secrets": [
      {
        "name": "google_refresh_token"
      },
      {
        "name": "google_access_token"
      },
      {
        "name": "google_client_id"
      },
      {
        "name": "google_client_secret"
      },
      {
        "name": "google_expires_at"
      }
    ],
    "storage": [
      {
        "name": "google_sheets_orders_id",
        "value": ""
      }
    ]
  },
  "files": [
    "shopify/order/send-to-google-sheets/in-orders-google-sheets.js",
    "shopify/order/send-to-google-sheets/out-orders-google-sheets.js"
  ]
}
