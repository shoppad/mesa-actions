{
  "key": "shoppad/mesa-templates/shopify/order/send-to-google-sheets-document",
  "name": "Send Order To Google Sheets Document",
  "version": "1.0.0",
  "description": "Send order from Shopify to Google Sheets document when order is created.",
  "tags": ["order", "google-sheets"],
  "source": "shopify",
  "destination": "google-sheets",
  "enabled": false,
  "config": {
    "inputs": [
      {
        "trigger_type": "input",
        "type": "shopify_webhook",
        "name": "Shopify Order Create",
        "key": "in-shopify-order-create",
        "script": "in-shopify-order-create.js",
        "topic": "orders/create"
      }
    ],
    "outputs": [
      {
        "trigger_type": "output",
        "type": "custom",
        "name": "Send to Google Sheets",
        "key": "out-send-to-google-sheets",
        "script": "out-send-to-google-sheets.js"
      }
    ],
    "secrets": [
      {
        "key": "google_refresh_token"
      },
      {
        "key": "google_access_token"
      },
      {
        "key": "google_client_id"
      },
      {
        "key": "google_client_secret"
      },
      {
        "key": "google_expires_at"
      }
    ],
    "storage": [
      {
        "key": "google_sheets_orders_id",
        "value": ""
      }
    ]
  }
}
