{
  "key": "shopify/order/tag_multiple_unfulfilled_orders_from_same_customer",
  "name": "Tag Multiple Unfulfilled Orders from the Same Customer",
  "version": "1.0.0",
  "description": "",
  "video": "",
  "readme": "",
  "tags": [],
  "source": "shopify",
  "destination": "filter",
  "enabled": false,
  "logging": false,
  "debug": false,
  "config": {
      "inputs": [
          {
              "schema": 2,
              "trigger_type": "input",
              "type": "shopify",
              "entity": "order",
              "action": "created",
              "name": "Shopify Order Created",
              "key": "shopify_order",
              "metadata": [],
              "weight": 0
          }
      ],
      "outputs": [
          {
              "schema": 2,
              "trigger_type": "output",
              "type": "filter",
              "name": "Filter",
              "key": "filter",
              "metadata": {
                  "a": "{{shopify_order.customer.last_order_id}}",
                  "comparison": "does not equal",
                  "b": "null"
              },
              "local_fields": [],
              "weight": 0
          },
          {
              "schema": 2,
              "trigger_type": "output",
              "type": "shopify",
              "entity": "order",
              "action": "retrieve",
              "name": "Shopify Retrieve Order",
              "key": "shopify_order_1",
              "metadata": {
                  "order_id": "{{shopify_order.customer.last_order_id}}",
                  "site": "current"
              },
              "local_fields": [],
              "weight": 1
          },
          {
              "schema": 2,
              "trigger_type": "output",
              "type": "filter",
              "name": "Filter",
              "key": "filter_1",
              "metadata": {
                  "a": "{{shopify_order_2.fulfillment_status}}",
                  "comparison": "does not equal",
                  "b": "fulfilled"
              },
              "local_fields": [],
              "weight": 2
          },
          {
              "schema": 3,
              "trigger_type": "output",
              "type": "shopify",
              "entity": "order",
              "action": "update",
              "name": "Shopify Update Order",
              "key": "shopify_order_4",
              "metadata": {
                  "order_id": "{{shopify_order.id}}",
                  "body": {
                      "tags": "{{shopify_order.tags}}, multiple orders unfulfilled"
                  },
                  "site": "current"
              },
              "local_fields": [],
              "weight": 3
          },
          {
              "schema": 3,
              "trigger_type": "output",
              "type": "shopify",
              "entity": "order",
              "action": "update",
              "name": "Shopify Update Order",
              "key": "shopify_order_2",
              "metadata": {
                  "site": "current"
              },
              "weight": 4
          }
      ],
      "storage": []
  }
}