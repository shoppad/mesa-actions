{
    "key": "uploadery/order/send_file_to_google_drive",
    "name": "Send Uploadery files to Google Drive",
    "version": "1.0.0",
    "enabled": false,
    "setup": true,
    "config": {
        "inputs": [
            {
                "schema": 2,
                "trigger_type": "input",
                "type": "uploadery",
                "entity": "order",
                "action": "created",
                "name": "Order Created",
                "key": "uploadery-order-created",
                "operation_id": "order_created",
                "metadata": [],
                "local_fields": [],
                "weight": 0
            }
        ],
        "outputs": [
            {
                "schema": 5,
                "trigger_type": "output",
                "type": "loop",
                "version": "v2",
                "entity": "loop",
                "operation_id": "loop_loop",
                "name": "Loop",
                "key": "loop",
                "metadata": {
                    "key": "{{uploadery-order-created.fields[]}}"
                },
                "local_fields": [],
                "weight": 0
            },
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "googledrive",
                "entity": "file",
                "action": "save",
                "name": "Save File",
                "key": "googledrive-save-file",
                "operation_id": "file_save",
                "metadata": {
                    "file_url": "{{loop.value}}",
                    "file_name": "{{uploadery-order-created.order.name}} - {{filename}}"
                },
                "local_fields": [],
                "weight": 1
            }
        ]
    }
}